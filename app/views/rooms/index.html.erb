<div class="container">
  <h5> Hi <%= @current_user.username %> </h5>
<h4> Users </h4>
<ul>
  <% @users.each do |user| %>
    <li>
        <%= user.username %>
    </li>
  <% end %>
</ul>
<h4> Rooms </h4>
<ul>
  <% @rooms.each do |room| %>
    <li>
      <a href="/rooms/<%= room.id %>">
        <%= room.name %>
      </a>
    </li>
  <% end %>
</ul>
</div>
